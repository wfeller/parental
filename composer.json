{
    "name": "wfeller/parental",
    "description": "A simple eloquent trait that allows relationships to be accessed through child models.",
    "license": "MIT",
    "authors": [
        {"name": "Caleb Porzio", "email": "calebporzio@gmail.com"},
        {"name": "William", "homepage": "https://github.com/wfeller"}
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "hanneskod/classtools": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "WF\\Parental\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WF\\Parental\\Tests\\": "tests/",
            "Database\\Factories\\": "tests/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WF\\Parental\\Providers\\ParentalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
