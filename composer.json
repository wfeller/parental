{
    "name": "tightenco/parental",
    "description": "A simple eloquent trait that allows relationships to be accessed through child models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Caleb Porzio",
            "email": "calebporzio@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^5.8|^6.0",
        "hanneskod/classtools": "~1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\Parental\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tightenco\\Parental\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\Parental\\Providers\\ParentalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
